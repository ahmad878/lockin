{
  "appId": "com.lockin.app",
  "appName": "Lock In",
  "webDir": "public",
  "server": {
    "cleartext": true,
    "allowNavigation": [
      "lockin-production.up.railway.app"
    ]
  },
  "plugins": {
    "CapacitorHttp": {
      "enabled": true
    },
    "LocalNotifications": {
      "smallIcon": "ic_stat_icon_config_sample",
      "iconColor": "#488AFF",
      "sound": "beep"
    },
    "PushNotifications": {
      "presentationOptions": ["badge", "sound", "alert"]
    }
  }
}